package Adapter;

public class Transfer {
	private double price;
	private String bank;
	public Transfer(double price, String bank) {
		super();
		this.price = price;
		this.bank = bank;
	}
	public double getPrice() {
		return price;
	}
	public void setPrice(double price) {
		this.price = price;
	}
	public String getBank() {
		return bank;
	}
	public void setBank(String bank) {
		this.bank = bank;
	}
	
	
	
}
